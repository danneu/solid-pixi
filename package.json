{
  "name": "@danneu/solid-pixi",
  "version": "0.0.1",
  "description": "A library for declaratively using pixi.js from solidjs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/danneu/solid-pixi.git"
  },
  "author": "danneu",
